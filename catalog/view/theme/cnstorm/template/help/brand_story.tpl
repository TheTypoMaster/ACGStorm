<script type="text/javascript">

var n = $(".team_member .our_team ul:visible").index();
var m = $(".team_member .our_team ul:last").index();

function prevGO() {
  $(".team_member .our_team ul").hide();
  if (n > 0) {
      $(".team_member .our_team ul").eq(n - 1).show();
      n = n - 1;
      $(".team_member .dots li").removeClass("after").eq(n).addClass("after");
  } else {
      $(".team_member .our_team ul:last").show();
      n = m;
      $(".team_member .dots li").removeClass("after").eq(n).addClass("after");
  }
}

function nextGo() {
  $(".team_member .our_team ul").hide();
  if (n < m) {
      $(".team_member .our_team ul").eq(n + 1).show();
      n = n + 1;
      $(".team_member .dots li").removeClass("after").eq(n).addClass("after");
  } else {
      $(".team_member .our_team:visible ul:first").show();
      n = 0;
      $(".team_member .dots li").removeClass("after").eq(n).addClass("after");
  }
}



$(".dots").each(function(i){
  var myThis = $(this).find("li");
  myThis.click(function(){
    $(this).addClass("after").siblings().removeClass("after");
    $(this).parent().prev().find("ul").eq($(this).index()).show().siblings().hide();
  });
})



</script>
