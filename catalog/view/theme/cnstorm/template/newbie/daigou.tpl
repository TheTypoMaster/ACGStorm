  }
}

function nextGroup() {
  $(".nav-box .cur_recommend .cur_ul:visible ul").hide();
  if (n < m) {
      $(".nav-box .cur_recommend .cur_ul:visible ul").eq(n + 1).show();
      n = n + 1;
      $(".nav-box .cur_recommend:visible .img_numNav li").removeClass("activeNS").eq(n).addClass("activeNS");
  } else {
      $(".nav-box .cur_recommend .cur_ul:visible ul:first").show();
      n = 0;
      $(".nav-box .cur_recommend:visible .img_numNav li").removeClass("activeNS").eq(n).addClass("activeNS");
  }
}

$(".websites_tap li").each(function(i){
  $(this).click(function() {
    $(this).addClass("on").siblings().removeClass("on");
    $(".cur_recommend").eq(i).show().siblings().hide();
     m = $(".nav-box .cur_recommend:visible .cur_ul ul:last").index();
  });
});

$(".img_numNav").each(function(i){
  var myThis = $(this).find("li");
  myThis.click(function(){
    $(this).addClass("activeNS").siblings().removeClass("activeNS");
    $(this).parent().prev().find("ul").eq($(this).index()).show().siblings().hide();
  });
})
</script> 
